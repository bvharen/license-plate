{
    "name": "bvanharen/license-plate",
    "type": "library",
    "description": "Autofill dashes for dutch license plates",
    "keywords": [
        "bvanharen",
        "license-plate"
    ],
    "homepage": "https://github.com/bvanharen/license-plate",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian van Haren",
            "email": "info@brianvanharen.nl",
            "homepage": "https://github.com/bvanharen",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "8"
    },
    "autoload": {
        "psr-4": {
            "Bvanharen\\LicensePlate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bvanharen\\LicensePlate\\Tests\\": "test"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
